[
    {
        "key": "ctrl+shift+/",
        "command": "extension.multiCommand.execute",
        "args": {
            "sequence": [
                "cursorEnd",
                {
                    "command": "type",
                    "args": {
                        "text": ";"
                    }
                },
                "cursorDown"
            ]
        },
        "when": "editorTextFocus"
    }
]